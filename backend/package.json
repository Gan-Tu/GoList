{
  "name": "golist-backend",
  "version": "0.1.0",
  "description": "Quickly browse a list of links and files with one simple URL",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "NODE_ENV=development tsc && cp -r ./views ./dist/views && cp -r ./public ./dist/public",
    "start": "PORT=8080 node ./dist/server.js",
    "dev": "NODE_ENV=development nodemon server.ts",
    "test": "npm run build && echo 'build successful'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gan-Tu/GoList.git"
  },
  "author": "Gan Tu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Gan-Tu/GoList/issues"
  },
  "homepage": "https://github.com/Gan-Tu/GoList#readme",
  "dependencies": {
    "@google-cloud/firestore": "^4.12.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "express": "^4.16.1",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.4.1",
    "http-errors": "^1.6.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "pug": "^3.0.1",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "npm": "^7.17.0",
    "ts-node": "^10.0.0"
  }
}
