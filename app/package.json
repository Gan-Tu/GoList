{
  "name": "golist-app-service",
  "version": "2.3.0",
  "description": "Quickly browse a list of links and files with one simple URL",
  "scripts": {
    "server": "nodemon ./bin/www",
    "client": "npm start --prefix client",
    "prestart": "npm run build",
    "start": "PORT=8080 node ./bin/www",
    "dev": "concurrently 'npm run server' 'npm run client'",
    "build": "npm run-script build --prefix client",
    "preversion": "npm --no-git-tag-version version minor --prefix client",
    "postversion": "git push && git push --tags",
    "bump-version": "npm version minor -m 'bump version to %s for deployment'",
    "deploy": "npm run bump-version && npm run build && gcloud app deploy",
    "deploy-current": "npm run build && gcloud app deploy",
    "deploy-staging": "npm run bump-version && npm run build && gcloud app deploy --no-promote",
    "test": "npm run build && echo 'build successful'",
    "postinstall": "npm install --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michael-Tu/GoList.git"
  },
  "author": "Gan Tu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Michael-Tu/GoList/issues"
  },
  "homepage": "https://github.com/Michael-Tu/GoList#readme",
  "dependencies": {
    "@google-cloud/datastore": "^6.3.1",
    "body-parser": "^1.19.0",
    "client": "file:./client",
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-rate-limit": "^5.2.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.7",
    "pug": "^3.0.0"
  }
}
