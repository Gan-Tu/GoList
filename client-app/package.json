{
  "name": "golist-client-app-service",
  "version": "0.2.0",
  "description": "Quickly browse a list of links and files with one simple URL",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf ./build && NODE_ENV=production next build",
    "prestart": "npm run build",
    "start": "next start -p 8080",
    "bump-version": "npm version minor -m 'bump version to %s for deployment'",
    "deploy": "npm run build && gcloud app deploy"
  },
  "dependencies": {
    "next": "^10.0.0",
    "react": "17.0.1",
    "react-bootstrap": "^1.5.0",
    "react-dom": "17.0.1",
    "swr": "^0.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michael-Tu/GoList.git"
  },
  "author": "Gan Tu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Michael-Tu/GoList/issues"
  },
  "homepage": "https://github.com/Michael-Tu/GoList#readme"
}
